{
  "NODE_ENV": "development",
  "TZ": "UTC",

  "port": 3000,
  "pageSize": {
    "default": 10
  },
  "limit": {
    "type": "number",
    "default": "100"
  },
  "aspectRatio": {
    "type": "number"
  },

  "maxSessionLength": {
    "type": "function",
    "default": "60 * 60 * 1000"
  },
  "maxExtendedSessionLength": {
    "type": "function",
    "default": "30 * 24 * this.maxSessionLength"
  },

  "minDate": {
    "type": "date",
    "default": "1899-12-31"
  },
  "version": {
    "type": "function",
    "default": "new Date().getTime()"
  },

  "sessionSecret": {
    "development": "secret key",
    "required": true
  },

  "logLevel": {
    "development": 3,
    "test": 2,
    "production": 1,
    "default": 0
  }
}
